# Environment variables for production deployment
# Copy this file to .env or .env.local for development

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Backend API Configuration
NEXT_PUBLIC_BACKEND_URL=https://your-backend-api.vercel.app
BE_URL=https://your-backend-api.vercel.app

# Authentication Endpoints (Optional - can use NEXT_PUBLIC_BACKEND_URL)
LOGIN_URL=https://your-backend-api.vercel.app/api/auth/login
REG_URL=https://your-backend-api.vercel.app/api/auth/register

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://your-frontend.vercel.app

# Environment
NODE_ENV=production

# Optional: Analytics
# NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX
# NEXT_PUBLIC_SENTRY_DSN=https://xxxxx@sentry.io/xxxxx
